{
  "name": "michaeljennings/utilities",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Jennings",
      "email": "michael.jennings91@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": ">=5.1"
  },
  "require-dev": {
    "illuminate/cache": ">=5.1",
    "michaeljennings/broker": "^1.0",
    "michaeljennings/refinery": "^1.2",
    "orchestra/testbench": ">=3.6"
  },
  "autoload": {
    "psr-4": {
      "MichaelJennings\\Utilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelJennings\\Utilities\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "MichaelJennings\\Utilities\\UtilityServiceProvider"
      ]
    }
  }
}
